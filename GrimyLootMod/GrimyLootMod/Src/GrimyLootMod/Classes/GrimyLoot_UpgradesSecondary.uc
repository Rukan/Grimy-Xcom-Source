static function X2DataTemplate CreateBasicGrimyDodgePiercePistol()
{
	local X2WeaponUpgradeTemplate Template;
	`CREATE_X2TEMPLATE(class'X2WeaponUpgradeTemplate', Template, 'GrimyDodgePierce_BscPistol');
	class'GrimyLoot_UpgradesPrimary'.static.SetUpScopeGraphics(Template, true);
	class'GrimyLoot_UpgradesPrimary'.static.SetUpTier1Upgrade(Template);
	Template.BonusAbilities.AddItem('GrimyDodgePierce_Bsc');
	Template.MutuallyExclusiveUpgrades.AddItem('GrimyDodgePierce_BscPistol');
	Template.MutuallyExclusiveUpgrades.AddItem('GrimyDodgePierce_AdvPistol');
	Template.MutuallyExclusiveUpgrades.AddItem('GrimyDodgePierce_SupPistol');
	Template.strImage = "img:///UILibrary_StrategyImages.X2InventoryIcons.ConvAssault_OpticC_inv";
	return Template;
}

